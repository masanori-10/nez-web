
File                 = CommaSeparatedValue
CommaSeparatedValue  = { $(Line) ( NEWLINE $(Line) )* !. #Top }
Line                 = { $(Value) ( ',' $(Value) )* #List }
Value                = '"' { ( '""' / !'"' . )* #String } '"'
                     / { ( ![,\r\n] . )* #Value }
NEWLINE              = '\r\n'
                     / '\n'
